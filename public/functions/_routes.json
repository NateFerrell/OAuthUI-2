{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/_*",
    "/favicon.ico",
    "/scripts/*"
  ],
  "routes": [
    {
      "pattern": "/api/auth/*",
      "function": "api/auth"
    },
    {
      "pattern": "/api/stockx/*",
      "function": "api/stockx"
    },
    {
      "pattern": "/callback",
      "function": "callback"
    }
  ]
}